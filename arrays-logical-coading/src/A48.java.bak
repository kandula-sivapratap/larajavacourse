class A48 
{
	public static void main(String[] args) 
	{
		int e[]={1,2,3,4,5,6,7,8};
		System.out.println("Intial");
		for (int i=0;i<=e.length-1 ;i++ )
		{
			System.out.print(e[i]+" ");
		}
		System.out.println();
		System.out.println("Final");
		for (int i=0;i<=e.length-1 ;i++ )
		{
			System.out.print(e[i]+5+" ");
		}
		System.out.println("Done!");
	}
}